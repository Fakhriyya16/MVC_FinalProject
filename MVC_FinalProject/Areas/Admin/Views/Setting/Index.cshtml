@model List<Setting>
@{
    ViewData["Title"] = "Index";
}

<section id="setting-table">
    <div class="container">
        <table class="table">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Key</th>
                    <th scope="col">Value</th>
                    <th scope="col">Created Date</th>
                    <th scope="col">Operations</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr class="setting-data">
                        <th scope="row">@item.Id</th>
                        <td>@item.Key</td>
                        <td>@item.Value</td>
                        <td>@item.CreatedDate.ToString("dd.MM.yyyy")</td>
                        <td>
                            <a type="button" asp-controller="Setting" asp-action="Edit" asp-route-id="@item.Id" class="btn btn-warning btn-sm">Edit</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</section>

